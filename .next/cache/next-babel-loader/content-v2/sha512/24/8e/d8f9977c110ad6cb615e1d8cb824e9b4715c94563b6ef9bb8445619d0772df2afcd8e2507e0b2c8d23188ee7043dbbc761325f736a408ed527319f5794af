 = (children, id) => {\r\n        setShowBack(true)\r\n        setContent(() => {\r\n            return courses.filter(el => Number(el.course_id) === id)\r\n        })\r\n        setCoursesData([courses.find(el => Number(el.id) === id)])\r\n    }\r\n    const handleContent = (id) => {\r\n        router.push(`/ru/office/modules?course_id=${id}`)\r\n    }\r\n    const back = () => {\r\n        setCoursesData(parentCourse)\r\n        setContent([])\r\n        setShowBack(false)\r\n    }\r\n    return (\r\n        <div >\r\n            <div>\r\n                {showBack && <div onClick={() => back()}>Назад</div>}\r\n            </div>\r\n            <div className={classes.courseWrapper}>\r\n                <div className={classes.courseParent}>\r\n                {\r\n                    coursesData && coursesData.map((el, idx) => (\r\n                        <div\r\n                            key={`courses${idx}`}\r\n                            onClick={() => handleCourses(el.course_id, el.id)}\r\n                        >{el.title}</div>\r\n                    ))\r\n                }\r\n                </div>\r\n                <div>\r\n                    {\r\n                        content.length > 0 && (\r\n                            content.map((elem, idx) => (\r\n                                <div\r\n                                    key={`contentCourse${idx}`}\r\n                                    onClick={() => handleContent(elem.id)}\r\n                                >{elem.title}</div>\r\n                            ))\r\n                        )\r\n                    }\r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\nexport default CoursesView"]},"metadata":{},"sourceType":"module"}